{
  "proxy_host": null,
  "proxy_port": null,
  "jenkins_host": "http://ci.theforeman.org/",
  "jenkins_url": "http://ci.theforeman.org/",
  "hub_config": "~/.config/hub",
  "github_user": "theforeman",
  "bot_github_user": "<%= @github_user -%>",
  "irc_channel": "#theforeman-dev",
  "not_mergable_cache": "~/pr_tests/cache/test_pull_request_smart_proxy_not_mergable",
  "repo_groups": {},
  "settings": {
    "test_settings": {
      "name": "test",
      "test_prefix": "Test Results:",
      "allow_multiple": true,
      "run_without_trigger": true,
      "branches": {
        "*": {
          "jenkins_job_name": "test_proxy_develop_pull_request",
          "downstream_job_name": "test_proxy_develop",
          "build_token": "<%= @jenkins_build_token -%>"
        }
      },
      "repo_to_orgs": {
        "smart-proxy": ["theforeman", "katello"]
      },
      "repo_to_pull_id_param": {
        "smart-proxy": {
          "git_url": "pr_git_url",
          "ref": "pr_git_ref",
          "number": "pr_number",
          "base_branch": "branch"
        }
      }
    }
  }
}
