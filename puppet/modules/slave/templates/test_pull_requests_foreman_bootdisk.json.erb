{
    "proxy_host": null,
    "proxy_port": null,
    "jenkins_host": "http://ci.theforeman.org/",
    "jenkins_url": "http://ci.theforeman.org/",
    "hub_config": "~/.config/hub",
    "github_user": "theforeman",
    "bot_github_user": "<%= @github_user -%>",
    "irc_channel": "#theforeman-dev",
    "not_mergable_cache": "~/pr_tests/cache/test_pull_request_foreman_bootdisk_not_mergable",
    "repo_groups": {
        "foreman_bootdisk": "foreman_bootdisk"
    },
    "settings": {
        "test_settings": {
            "name": "test",
            "test_prefix": "Test Results:",
            "allow_multiple": true,
            "run_without_trigger": true,
            "branches": {
                "*": {
                    "jenkins_job_name": "test_plugin_pull_request",
                    "downstream_job_name": "test_plugin_foreman_bootdisk_master",
                    "require_downstream_stability": false,
                    "build_token": "<%= @jenkins_build_token -%>",
                    "addtl_jenkins_params": {
                        "foreman_bootdisk": {
                            "plugin_repo": "https://github.com/theforeman/foreman_bootdisk.git",
                            "plugin_name": "foreman_bootdisk"
                        }
                    }
                }
            },
            "repo_to_orgs": {
                "foreman_bootdisk": [
                    "theforeman",
                    "katello"
                ]
            },
            "repo_to_pull_id_param": {
                "foreman_bootdisk": {
                    "git_url": "pr_git_url",
                    "ref": "pr_git_ref",
                    "number": "pr_number",
                    "base_branch": "plugin_branch"
                }
            }
        }
    }
}
